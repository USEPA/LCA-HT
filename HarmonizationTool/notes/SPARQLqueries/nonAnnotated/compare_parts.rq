#

PREFIX fedlca: <http://epa.gov/nrmrl/std/lca/fedlca/1.0#>
PREFIX lcaht: <http://epa.gov/nrmrl/std/lca/ht/1.0#>
PREFIX olca: <http://openlca.org/schema/v1.0/>
PREFIX eco: <http://ontology.earthster.org/eco/core#>
PREFIX fasc: <http://ontology.earthster.org/eco/fasc#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX afn: <http://jena.hpl.hp.com/ARQ/function#>
PREFIX fn: <http://www.w3.org/2005/xpath-functions#>
PREFIX xml: <http://www.w3.org/XML/1998/namespace>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX dcterms: <http://purl.org/dc/terms/>

select distinct ?oFlow ?fn (str(?masterFlowableName) as ?mfn)
  where {

  ?comp a fedlca:Comparison .
  ?comp fedlca:comparedSource ?sampleFlowable .
  ?comp fedlca:comparedMaster ?masterFlowable .
  ?comp fedlca:comparedEquivalence fedlca:Equivalent .
  ?sampleFlowable a eco:Flowable .
  ?sampleFlowable rdfs:label ?flowableName .

  ?flow eco:hasFlowable ?flowable .
  ?masterFlowable rdfs:label ?masterFlowableName .
   filter (fn:lower-case(?masterFlowableName) != fn:lower-case(?flowableName))
  bind(str(?flowableName) as ?fn)
  ?oFlow olca:name ?fn .
  #filter regex (?fn, "^b","i")
  ?oFlow a olca:Flow .
  ?oFlow olca:name ?fn .
}
