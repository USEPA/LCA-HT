###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#flowContextForbiddenRegexPattern

:flowContextForbiddenRegexPattern rdf:type owl:DatatypeProperty ;
                                  
                                  rdfs:label "forbidden pattern:"@en ;
                                  
                                  rdfs:domain :FlowContext ;
                                  
                                  rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#flowContextGeneral

:flowContextGeneral rdf:type owl:DatatypeProperty ;
                    
                    rdfs:label "General"@en ;
                    
                    rdfs:domain :FlowContext ;
                    
                    rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#flowContextNecessaryRegexPattern

:flowContextNecessaryRegexPattern rdf:type owl:DatatypeProperty ;
                                  
                                  rdfs:label "required pattern:"@en ;
                                  
                                  rdfs:comment "When user data are matched to master lists, regular expressions may aid in determining a match.  The string found in the user data must matches this \"flowContextNecessaryRegexPattern\" regular expression, for the given master Flow Context."@en ;
                                  
                                  rdfs:domain :FlowContext ;
                                  
                                  rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#flowContextSpecific

:flowContextSpecific rdf:type owl:DatatypeProperty ;
                     
                     rdfs:label "Specific"@en ;
                     
                     rdfs:domain :FlowContext ;
                     
                     rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#flowContextSufficientRegexPattern

:flowContextSufficientRegexPattern rdf:type owl:DatatypeProperty ;
                                   
                                   rdfs:label "required are one or more patterns like:"@en ;
                                   
                                   rdfs:comment "When user data are matched to master lists, regular expressions may aid in determining a match.  If the string found in the user data matches  AT LEAST ONE  \"flowContextSufficientRegexPattern\" regular expression, for the given master Flow Context, the user data is assigned to this Flow Context -- UNLESS it fails one or more \"flowContextNecessaryRegexPattern\" regular expressions."@en ,
                                                "When user data are matched to master lists, regular expressions may aid in determining a match.  If the string found in the user data matches AT LEAST ONE \"flowContextSufficientRegexPattern\" regular expression, for the given master Flow Context, the user data is assigned to this Flow Context -- UNLESS it fails one or more \"flowContextNecessaryRegexPattern\" regular expressions."@en ;
                                   
                                   rdfs:domain :FlowContext ;
                                   
                                   rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#hasOpenLCAUUID

:hasOpenLCAUUID rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                
                rdfs:range xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#presentationSortIndex

:presentationSortIndex rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       
                       rdfs:domain :FlowContext ;
                       
                       rdfs:range xsd:integer ;
                       
                       rdfs:subPropertyOf owl:topDataProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#Emission

:Emission rdf:type owl:Class ;
          
          rdfs:label "Release to"@en ;
          
          rdfs:subClassOf :FlowContext ,
                          [ rdf:type owl:Class ;
                            owl:complementOf :Resource
                          ] .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#FlowContext

:FlowContext rdf:type owl:Class ;
             
             rdfs:label "Flow Context"^^xsd:string ;
             
             rdfs:subClassOf [ rdf:type owl:Class ;
                               owl:unionOf ( :Emission
                                             :Resource
                                           )
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :presentationSortIndex ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:int
                             ] ;
             
             rdfs:comment "The Flow Context is a term developed for the LCA Harmonization Tool.  It encompases terms such as 'Category' or 'Compartment' and may have several descriptors including geological feature, population density, or land use.  A Flow has a hasFlowContext property with an object being a FlowContext.  This term is similar to fasc:Compartment.  Examples of Flow Contexts include emissions to urban air and resource consumption from water."^^xsd:string .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#Resource

:Resource rdf:type owl:Class ;
          
          rdfs:label "Resource"@en ;
          
          rdfs:subClassOf :FlowContext ,
                          [ rdf:type owl:Class ;
                            owl:complementOf :Emission
                          ] .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#ToAir

:ToAir rdf:type owl:Class ;
       
       rdfs:label "air"@en ;
       
       rdfs:subClassOf :Emission .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#ToSoil

:ToSoil rdf:type owl:Class ;
        
        rdfs:label "soil"@en ;
        
        rdfs:subClassOf :Emission .



###  http://epa.gov/nrmrl/std/lca/fedlca/1.0#ToWater

:ToWater rdf:type owl:Class ;
         
         rdfs:label "water"@en ;
         
         rdfs:subClassOf :Emission .



###  http://epa.gov/nrmrl/std/lca/ht/1.0#MasterDataset

lcaht:MasterDataset rdf:type owl:Class .



###  http://www.w3.org/2002/07/owl#DataTypeProperty

owl:DataTypeProperty rdf:type owl:Class .





